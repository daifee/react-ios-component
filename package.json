{
  "name": "react-ios-component",
  "version": "1.0.0",
  "description": "根据 ios 组件的风格，使用 react 对组件进行封装以便可以重用",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --colors --port 8088 --config webpack.config.dev.js --host 0.0.0.0",
    "lint": "eslint ./",
    "lint:watch": "esw -w ./",
    "doc": "esdoc -c esdoc.json",
    "test": "npm run lint && mocha --opts test/mocha.opts",
    "test:watch": "mocha -w --opts mocha.opts",
    "test:karma": "npm run lint && karma start",
    "clear": "rm -rf ./dist",
    "build:dev": "NODE_ENV=development webpack --progress --colors --config webpack.config.prod.js",
    "build:prod": "NODE_ENV=production webpack -p --progress --colors --config webpack.config.prod.js",
    "build": "npm run clear && npm run build:dev && npm run build:prod"
  },
  "keywords": [
    "react",
    "component",
    "mobile",
    "ui"
  ],
  "author": "haw",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^2.11.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.4.0",
    "eslint-plugin-promise": "^1.3.1",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "eslint-watch": "^2.1.11",
    "extract-text-webpack-plugin": "^1.0.1",
    "fastclick": "^1.0.6",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.19.0",
    "jsdom": "^9.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.1",
    "node-sass": "^3.7.0",
    "open-browser-webpack-plugin": "0.0.2",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "iscroll": "^5.2.0",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-router": "^2.6.1"
  }
}
